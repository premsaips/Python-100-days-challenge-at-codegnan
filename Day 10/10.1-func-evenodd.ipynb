    "#Check weather the number is even or odd by using functions\n",
    "def evenodd(num):\n",
    "    if num % 2 == 0:\n",
    "       return True\n",
    "    else:\n",
    "        return False\n",
    "\n",
    "num = int(input())\n",
    "result =evenodd(num)\n",
    "if result == True:\n",
    "    print(num,\" is even.\")\n",
    "else:\n",
    "    print(num, \"is odd.\")\n"
   ]
